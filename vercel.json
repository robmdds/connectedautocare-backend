{
  "version": 2,
  "name": "connectedautocare-backend",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python@3.11.0"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "api"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}